	email = orrybaram@gmail.com
[alias]
    c = commit -am
    up = pull
    p = push
    s = status
    df = diff --color --color-words --abbrev
    lg = log --color --graph --pretty=format:'%Cblue%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --
[color]
    ui = always
[core]
    excludesfile = ~/.gitignore
    attributesfile = ~/.gitattributes
[color "branch"]
    current = yellow reverse
    local = yellow
    remote = green
[color "diff"]
    meta = yellow bold
    frag = magenta bold
    old = red bold
    new = green bold
[color "status"]
    added = yellow
    changed = green
    untracked = cyan
[merge]
    log = true
